# Using count() method

my_tuple = (1, 2, 3, 2, 2)
count = my_tuple.count(2)  
print(f"The count of 2 in my_tuple is: {count}")
print()

# Another example

# Creating a tuple with parentheses
tup1 = (10, 20, 30, 40, 50, 10, 10)
print("Tuple 1:", tup1)
print()

print("The count of 10 in tup1 is:", tup1.count(10))
print()
